{
  "ga4-server": {
        "source": {
          "type": "dom"
        },

      "destinations": [
          {
            "statements": [ 
                  {
                      "invoke": "window.dataLayer.push",
                      "with": [
                      {
                        "event": "evolv",
                        "evolvEventDetails": {
                          "cid": "${cid}",
                          "eid": "${eid}",
                          "excluded": false,
                          "group_id": "${group_id}",
                          "non_interaction": true,
                          "ordinal": "${combination_id}",
                          "projectName": "${project_name}",
                          "type": "confirmed",
                          "uid": "${uid}"
                        } 
                      }
                    ]
                  }
              ],
              "poll": {
                  "duration": 20000,
                  "interval": 50
              }
          }
      ]
  },
  "ga4": {
    "destinations": [
        {
          "statements": [ 
                {
                    "invoke": "window.dataLayer.push",
                    "with": [
                    {
                      "event": "evolv",
                      "evolvEventDetails": {
                        "cid": "${cid}",
                        "eid": "${eid}",
                        "excluded": false,
                        "group_id": "${group_id}",
                        "non_interaction": true,
                        "ordinal": "${combination_id}",
                        "projectName": "${project_name}",
                        "type": "confirmed",
                        "uid": "${uid}"
                      } 
                    }
                  ]
                }
            ],
            "poll": {
                "duration": 20000,
                "interval": 50
            }
        }
    ]
  }
}