{
  "segment": [
    {
      "statements": [
        {
          "invoke": "window.console.info",
          "with": [
            "********* Experiment Viewed *********",
            {
              "nonInteraction": 1,
              "experiment_id": "${group_id}",
              "experiment_name": "${project_name}",
              "variation_id": "${cid}",
              "variation_name": "combination ${combination_id}"
            }
          ]
        }
      ],
      "poll": {
        "duration": 20000,
        "interval": 50
      }
    }
  ],
  "uniqueConfirmationsPerSession": true
}
