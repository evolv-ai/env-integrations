{
    "name": "@evolv-delivery/catalyst",
    "version": "0.6.0",
    "description": "Provides a rendering SDK for experiments",
    "main": "dist/index.js",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "watch:rollup": "rollup -c --watch",
        "serve:catalyst": "set -m; http-server serve -p 3333 --cors --silent & echo $! > .catalystpid; fg > /dev/null",
        "serve:sandbox": "set -m; http-server cypress/sandbox -p 3334 --silent & echo $! > .sandboxpid; fg > /dev/null",
        "cleanup": "kill $(cat .sandboxpid) $(cat .catalystpid); rm .sandboxpid .catalystpid",
        "serve": "npm-run-all -p serve:*",
        "start": "npm-run-all -p watch:* serve:catalyst",
        "cypress": "npx wait-on http://localhost:3333 && npx wait-on http://localhost:3334 && cypress run --spec ./cypress/e2e/catalyst-test.cy.js; result=$?; npm run cleanup; exit $result",
        "test": "npm-run-all -s build -p serve cypress"
    },
    "keywords": [
        "render",
        "UI"
    ],
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/evolv-ai/env-integrations.git"
    },
    "homepage": "https://github.com/evolv-ai/env-integrations/catalyst",
    "bugs": {
        "url": "https://github.com/evolv-ai/env-integrations/issues"
    },
    "author": "Richard Cowin and Charles Robertson",
    "license": "ISC",
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-json": "^5.0.1",
        "cypress": "^12.2.0",
        "http-server": "^14.1.1",
        "npm-run-all": "^4.1.5",
        "rollup": "^3.2.3"
    }
}
